{
  "name": "framework-for-automation-integration-testing-petstore",
  "version": "1.0.0",
  "description": "develop framework for automation testing API petstore with TypeScript",
  "scripts": {
    "postinstall": "npm run pretest",
    "pretest": "openapi-typescript url/api/swagger.json --output ./.temp/types.ts",
    "test": "mocha --timeout=30000 --require ts-node/register --require dotenv/config ./tests/**/*.test.ts"
  },
  "keywords": [
    "testing",
    "framework",
    "automation",
    "mocha",
    "integration"
  ],
  "author": "Sasha Shulha (sashamiller666@gmail.com)",
  "license": "ISC",
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.0.2",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.0.1",
    "@types/tough-cookie": "^4.0.0",
    "ajv": "^8.2.0",
    "dotenv": "^9.0.2",
    "envalid": "^7.1.0",
    "got": "^11.8.2",
    "mocha": "^8.3.2",
    "openapi-typescript": "^3.2.3",
    "tough-cookie": "^4.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
